{
  "name": "my-site",
  "private": true,
  "version": "0.0.1",
  "description": "A starter project for AngularJS",
  "repository": "https://github.com/maxgardiner/personal-site",
  "license": "None",
  "devDependencies": {
    "http-server": "^0.9.0"
  },
  "dependencies": {
    "@bower_components/angular" : "angular/bower-angular#~1.5.0",
    "@bower_components/angular-loader" : "angular/bower-angular-loader#~1.5.0",
    "@bower_components/angular-mocks" : "angular/bower-angular-mocks#~1.5.0",
    "@bower_components/angular-route" : "angular/bower-angular-route#~1.5.0",
    "@bower_components/html5-boilerplate" : "h5bp/html5-boilerplate#^5.3.0"
  },
  "scripts": {
    "prestart": "sass --watch app/scss:app/css & yarn",
    "start": "http-server -a localhost -p 8000 -c-1 ./app && pkill ruby && pkill fsevent_watch",
    "postinstall" : "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'app/bower_components', 'junction') } catch (e) { }\""
  },
  "engines" : {
  "yarn" : ">= 1.0.0"
}
}
